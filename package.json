{
  "name": "blockapps-rest",
  "version": "7.0.1",
  "description": "BlockApps rest api",
  "main": "dist/index.js",
  "scripts": {
    "mocha-babel": "node_modules/.bin/mocha --require @babel/register",
    "build": "babel lib --out-dir dist/lib && babel util --out-dir dist/util",
    "prepublish": "yarn build",
    "test": "yarn test:lib && yarn test:util",
    "test:lib": "yarn mocha-babel lib/test/*.test.js -b",
    "test:util": "yarn mocha-babel util/test/*.test.js -b"
  },
  "keywords": [
    "BlockApps",
    "rest",
    "api"
  ],
  "author": "lior@blockapps.net",
  "license": "ISC",
  "engines": {
    "node": ">= 10.1.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bignumber.js": "2.4.0",
    "bluebird": "^3.4.6",
    "chai": "^4.2.0",
    "chai-bignumber": "^2.0.0",
    "commander": "^2.9.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "http-status-codes": "^1.3.0",
    "https": "1.0.0",
    "ip": "^1.1.5",
    "js-yaml": "3.6.1",
    "jsonwebtoken": "8.4.0",
    "query-string": "^6.2.0",
    "simple-oauth2": "2.2.1",
    "sol-merger": "^0.1.3",
    "solidity-parser": "^0.4.0",
    "solidity-parser-antlr": "^0.4.1",
    "soljitsu": "^1.0.0",
    "sync-request": "6.0.0",
    "tcp-port-used": "^1.0.0",
    "unix-time": "1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "mocha": "^6.0.2"
  }
